{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "Gyazoの履歴を保存します",
  "version": "2.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    },
    "default_title": "__MSG_browserActionTitle__",
    "default_popup": "pages/popup.html"
  },
  "options_page": "pages/options.html",
  "permissions": [
    "storage",
    "*://gyazo.com/*"
  ]
}
